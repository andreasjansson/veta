#!/bin/bash
# Stop the agent-sdk example workers
pkill -f "server.mjs.*agent-sdk" 2>/dev/null || true
pkill -f "wrangler.*veta-agent-example" 2>/dev/null || true
pkill -f "wrangler.*veta-example" 2>/dev/null || true
if [ -f /tmp/agent-sdk-test.pid ]; then
    kill $(cat /tmp/agent-sdk-test.pid) 2>/dev/null || true
    rm -f /tmp/agent-sdk-test.pid
fi
rm -f /tmp/agent-sdk-test.log
