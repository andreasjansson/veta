{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "veta-agent-example",
  "main": "src/agent.ts",
  "compatibility_date": "2025-01-01",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": "public"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "Chat",
        "class_name": "Chat"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["Chat"]
    }
  ],
  "services": [
    {
      "binding": "VETA",
      "service": "veta-example"
    }
  ]
}
